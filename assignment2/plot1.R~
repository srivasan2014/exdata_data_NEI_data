setwd("/home/srivasan/exdata_data_NEI_data")
nei<-readRDS("Source_Classification_Code.rds")
scc<-readRDS("summarySCC_PM25.rds")
a1<-subset(scc$Emission,scc$year==1999)
a2<-subset(scc$Emission,scc$year==2002)
a3<-subset(scc$Emission,scc$year==2005)
a4<-subset(scc$Emission,scc$year==2008)
z<-data.frame(emision1999=sum(a1),emision2002=sum(a2),emision2005=sum(a3),emision2008=sum(a4))
TOTAL_EMISSION_PM2.5<-list(sum(a1),sum(a2),sum(a3),sum(a4))
YEAR<-list("1999","2002","2005","2008")
png("plot1.png",width=480,height=480)
plot(YEAR,TOTAL_EMISSION_PM2.5,type="lines")
dev.off()